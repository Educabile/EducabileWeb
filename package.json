{
  "name": "educabile-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mdi/js": "^2.8.94",
    "@mdi/react": "^1.0.1",
    "axios": "^0.18.0",
    "class-names": "^1.0.0",
    "i18next": "^12.0.0",
    "i18next-browser-languagedetector": "^2.2.4",
    "i18next-chained-backend": "^0.1.2",
    "i18next-localstorage-backend": "^1.1.4",
    "i18next-xhr-backend": "^1.5.1",
    "materialize-css": "^1.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-i18next": "^8.3.8",
    "react-lazy-load": "^3.0.13",
    "react-materialize": "^2.4.5",
    "react-media": "^1.8.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.0",
    "react-router-transition": "^1.2.1",
    "react-scripts": "2.1.1",
    "react-scroll-up": "^1.3.3",
    "react-scroll-up-button": "^1.6.4",
    "react-scrollspy": "^3.3.5",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "craco start",
    "test": "craco test --env=jsdom",
    "build:staging": "env-cmd .env.staging craco build",
    "build:production": "env-cmd .env.production craco build",
    "lint:js": "eslint src/**/*.js",
    "lint:js:fix": "eslint src/**/*.js --fix",
    "lint:css": "stylelint src/**/*.css",
    "lint:css:fix": "stylelint src/**/*.css --fix",
    "pretty:js": "prettier --write src/**/*.js",
    "pretty:css": "prettier --write src/**/*.css",
    "assets": "run-s assets:resize assets:webp",
    "assets:resize": "env-cmd .env node src/libs/images/resizeAll.js",
    "assets:webp": "node src/libs/images/toWebP.js",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn pretty:js",
      "yarn lint:js:fix",
      "git add"
    ],
    "src/**/*.css": [
      "yarn pretty:css",
      "yarn lint:css:fix",
      "git add"
    ],
    "src/**/*.{jpg,jpeg,png}": [
      "yarn assets",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-react-constant-elements": "^7.0.0",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-angular": "^7.1.2",
    "@craco/craco": "^2.2.3",
    "babel-plugin-transform-imports": "^1.5.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.20",
    "conventional-changelog-cli": "^2.0.11",
    "env-cmd": "^8.0.2",
    "eslint-plugin-jest": "^22.0.0",
    "husky": "^1.1.4",
    "lint-staged": "^8.0.4",
    "npm-run-all": "^4.1.3",
    "postcss-assets": "^5.0.0",
    "postcss-brand-colors": "^3.0.0",
    "postcss-calc": "^7.0.1",
    "postcss-color-function": "^4.0.1",
    "postcss-convert-values": "^4.0.1",
    "postcss-easing-gradients": "^3.0.1",
    "postcss-font-magician": "^2.2.1",
    "postcss-modules-values-replace": "^3.0.1",
    "postcss-normalize-charset": "^4.0.1",
    "postcss-normalize-url": "^4.0.1",
    "postcss-ordered-values": "^4.1.1",
    "postcss-preset-env": "^6.4.0",
    "postcss-pseudo-content-insert": "^0.3.0",
    "postcss-unique-selectors": "^4.0.1",
    "postcss-will-change-transition": "^1.2.0",
    "prettier": "^1.15.2",
    "release-it": "^7.6.3",
    "sharp": "^0.21.0",
    "stylelint": "^9.8.0",
    "stylelint-a11y": "^1.1.9",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-csstree-validator": "^1.3.0",
    "stylelint-declaration-block-no-ignored-properties": "^1.1.0",
    "stylelint-group-selectors": "^1.0.7",
    "stylelint-high-performance-animation": "^1.1.1",
    "stylelint-images": "^2.0.0",
    "stylelint-no-indistinguishable-colors": "^1.2.1",
    "stylelint-order": "^1.0.0",
    "stylelint-selector-no-empty": "^1.0.2",
    "stylelint-value-no-unknown-custom-properties": "^2.0.0",
    "webpcss": "^1.2.2"
  },
  "browserslist": [
    "last 2 versions",
    ">.2%",
    "cover 90% in IT",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
